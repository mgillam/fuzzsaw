{
  "name": "fuzzsaw",
  "version": "1.0.0",
  "description": "A websocket fuzzing engine",
  "main": "./lib/app.js",
  "bin": {
    "fuzzsaw": "./lib/app.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "test": "sudo npm i -g && fuzzsaw",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgillam/fuzzsaw.git"
  },
  "keywords": [
    "fuzzing",
    "security",
    "websockets"
  ],
  "author": "mgillam <micgillam@gmail.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/mgillam/fuzzsaw/issues"
  },
  "homepage": "https://github.com/mgillam/fuzzsaw#readme",
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@types/socket.io-client": "^1.4.33",
    "@types/websocket": "^1.0.0",
    "@types/ws": "^7.2.5",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^5.1.0",
    "figlet": "^1.4.0",
    "path": "^0.12.7",
    "socket.io": "^2.4.0",
    "socket.io-client": "^2.3.0",
    "squirrelly": "^8.0.1",
    "websocket": "^1.0.31",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@types/clear": "^0.1.0",
    "@types/figlet": "^1.2.0",
    "@types/node": "^14.0.13",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
